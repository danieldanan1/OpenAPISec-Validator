{
  "rules": [{
  "or": [
    "security,exist,fail_msg=,success_msg=%^%^success",
    {
      "all": "paths.#path",
      "rule": {
        "condition": "paths.$path.security,not_exist",
        "true": {
          "all": "paths.$path.#method",
          "rule": "paths.$path.$method.security,exist",
          "fail_msg": "!@!@"
        }
      }
    }
  ]
}]
}
